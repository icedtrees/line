{
  "manifest_version": 2,

  "name": "Cats",
  "description": "Cats are good",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["line.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}

